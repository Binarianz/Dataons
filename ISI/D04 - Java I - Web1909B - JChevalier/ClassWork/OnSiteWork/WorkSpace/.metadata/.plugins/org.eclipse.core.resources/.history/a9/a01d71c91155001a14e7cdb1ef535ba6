package com.isi.boat.Controllers;
import javax.swing.*;

public class JavaJOptionPaneWarningMessage {

	JFrame f;

	JavaJOptionPaneWarningMessage() {
		f = new JFrame();
		JOptionPane.showMessageDialog(f, "Error in Upload.", 
				"Alert", JOptionPane.WARNING_MESSAGE);
	}

	public static void main(String[] args) {
		new JavaJOptionPaneWarningMessage();
	}
}