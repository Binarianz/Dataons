import java.util.ArrayList;

public class Burger {
	
	
	private String name;
	private Double price;
	private String roll;
	private String meat;
	private ArrayList <Topping> toppings= new ArrayList<>();
	
	public Burger(String _name,Double _price,String _roll,String _meat) {
		this.name=_name;
		this.price=_price;
		this.roll=_roll;
		this.meat=_meat;
		
	}
	
	
	
	
	
	
	

}
